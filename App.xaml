<Application
    x:Class="Default_WPF_MVVM_Pattern_Implemented.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Default_WPF_MVVM_Pattern_Implemented"
    xmlns:viewmodels="clr-namespace:Default_WPF_MVVM_Pattern_Implemented.ViewModels"
    xmlns:views="clr-namespace:Default_WPF_MVVM_Pattern_Implemented.Views"
    d1p1:Ignorable="d">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:SecondPageViewModel}">
            <views:SecondPageUserControl />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:MainWindowModel}">
            <local:MainWindow />
        </DataTemplate>

    </Application.Resources>
</Application>